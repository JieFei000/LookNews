/* pages/home/home.wxss */
.nav{
  height: 13vw;
  overflow: auto;
  white-space: nowrap;
  background-color: #fff;
}
.swiper{
  height: 60vw;
}
.img{
  width: 100vw;
  height: 60vw;
}
.nav_list{
  display: inline-block;
  width: 20vw;
  text-align: center;
  height: 13vw;
  line-height: 13vw;
}
.color{
  color: red;
}
.title{
  position: absolute;
  bottom: 0;
  left: 0;
  color: #fff;
  background-color: #000;
  width: 100vw;
  font-size: 5.5vw;
  padding: 1vw 5vw;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.toTop{
  width: 10vw;
  height: 10vw;
  font-size: 10vw;
  position: fixed;
  right: 10vw;
  bottom: 30vw;
  color: red;
  z-index: 999;
}